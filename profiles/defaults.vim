" some defaults settings
syntax on
set ls=2
set noai
set mouse=a
set hlsearch
set autoread
set incsearch
set expandtab
set tabstop=4
set noerrorbells
set shiftwidth=4
filetype plugin on
set visualbell t_vb=
set ignorecase smartcase
set statusline=%F%m%r%h%w\ format=%{&ff}\ type=%Y\ x=%l\ y=%v\ %p%%\ %{strftime(\"%d/%m/%y\ -\%H:%M\")}

set directory=~/.vim/swp/
